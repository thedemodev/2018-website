{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "gatsby develop",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop"],
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["--nolazy"],
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "gatsby:*"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "outFiles": []
    }
  ]
}
