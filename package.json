{
  "private": true,
  "name": "@undataforum/2018-website",
  "description": "Website for the 2018 UN Data Forum.",
  "version": "0.0.0",
  "author": {
    "name": "Thilo Maier",
    "email": "maier@un.org"
  },
  "homepage": "https://2018.undataforum.org",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "rimraf ./public && gatsby build",
    "deploy": "yarn run build && gh-pages -d ./public",
    "format": "pretty-quick",
    "inspect": "node --inspect-brk node_modules/.bin/gatsby develop",
    "lint": "eslint --ignore-path .gitignore .",
    "precommit": "pretty-quick --staged",
    "release": "release",
    "postrelease": "yarn run deploy"
  },
  "dependencies": {
    "@undataforum/components": "^0.7.0",
    "gatsby": "^2.0.0-beta.29",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.3",
    "gatsby-plugin-sharp": "^2.0.0-beta.3",
    "gatsby-remark-autolink-headers": "^2.0.0-beta.3",
    "gatsby-remark-smartypants": "^2.0.0-beta.2",
    "gatsby-source-filesystem": "^2.0.1-beta.3",
    "gatsby-transformer-remark": "^2.1.1-beta.3",
    "gatsby-transformer-sharp": "^2.1.1-beta.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "rehype-react": "^3.0.3",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "prettier": "^1.13.7",
    "pretty-quick": "^1.6.0",
    "release": "^4.0.1",
    "rimraf": "^2.6.2"
  }
}
